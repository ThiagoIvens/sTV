<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="th.sTV.updateMusicController">
   <children>
      <VBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane VBox.vgrow="ALWAYS">
               <children>
                  <Label alignment="CENTER" text="Atualizar Musica" textFill="WHITE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
            <AnchorPane maxHeight="300.0" maxWidth="500.0" VBox.vgrow="ALWAYS">
               <children>
                  <TextField fx:id="txtId" maxHeight="50.0" maxWidth="300.0" promptText="Id" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="50.0">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </AnchorPane>
            <AnchorPane maxHeight="300.0" maxWidth="500.0" VBox.vgrow="ALWAYS">
               <children>
                  <TextField fx:id="txtNome" maxHeight="50.0" maxWidth="300.0" promptText="Nome" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="50.0">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </AnchorPane>
            <AnchorPane maxHeight="300.0" maxWidth="500.0" VBox.vgrow="ALWAYS">
               <children>
                  <TextField fx:id="txtAutor" maxHeight="50.0" maxWidth="300.0" promptText="Autor" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="50.0">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </AnchorPane>
            <AnchorPane maxHeight="300.0" maxWidth="500.0" VBox.vgrow="ALWAYS">
               <children>
                  <TextField fx:id="txtLink" maxHeight="50.0" maxWidth="300.0" promptText="link" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="50.0">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </AnchorPane>
            <AnchorPane VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="btnAdd" alignment="CENTER" contentDisplay="CENTER" maxHeight="50.0" maxWidth="100.0" mnemonicParsing="false" onAction="#addDataBase" style="-fx-background-color: #FFFAFA;" text="Atualizar" AnchorPane.leftAnchor="0.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
